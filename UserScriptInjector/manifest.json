{
  "manifest_version": 3,
  "name": "UserScript Injector",
  "version": "1.0",
  "description": "Extensão para injetar scripts personalizados em sites específicos.",
  "permissions": ["scripting", "activeTab", "storage"],
  "host_permissions": ["https://google.com/*"],
  "action": {
    "default_popup": "popup/popup.html",
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  }
}
